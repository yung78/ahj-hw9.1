{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": 0,
        "no-constant-condition": 0,
        "no-underscore-dangle": 0,
        "class-methods-use-this": 0,
        "no-unneeded-ternary": 0,
        "import/extensions": 0,
        "no-unused-expressions": 0,
        "no-param-reassign": 0,
        
        "no-console": 1,
        "no-alert": 1,
        "operator-linebreak": 1,
        "no-restricted-syntax": 1,
        "no-return-assign": 1
    }
}
